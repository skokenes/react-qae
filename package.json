{
  "name": "react-qae",
  "version": "0.0.1",
  "description": "React integration for the Qlik Associative Engine",
  "main": "dist/react-qae.js",
  "module": "dist/esm/index.js",
  "es2015": "dist/esm/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
